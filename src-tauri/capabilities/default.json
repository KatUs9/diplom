{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*:*"
        },
        {
          "url": "http://*:*"
        }
      ]
    },
    {
      "identifier": "fs:default",
      "allow": [
        { "path": "/etc/schedule/config.toml" },
        { "path": "C:\\\\ProgramData\\Schedule\\config.toml" }
      ]
    },
    "os:default"
  ]
}
